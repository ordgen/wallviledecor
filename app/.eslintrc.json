{
  "extends": [
    "airbnb", 
    "plugin:meteor/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "meteor",
    "react"
  ],
  "rules": {
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0
	},
	"parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "window": true
  }
}